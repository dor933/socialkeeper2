{
  "expo": {
    "name": "social-keeper-client",
    "slug": "social-keeper-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/RandomImages/SocialKeeper.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "plugins":[
      [
        "expo-image-picker",
        {
   
          "photosPermission": "The app accesses your photos to let you share them with your friends."



        }
      ] ,
      [
        "expo-calendar",
        {
          "calendarPermission": "The app accesses your calendar to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true ,
      "config": {
        "googleMapsApiKey:": "AIzaSyD-9tSrke72PouQMnMX-a7eZSW0jkFMBWY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD-9tSrke72PouQMnMX-a7eZSW0jkFMBWY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "socialkeeperclient://+/redirect"
  }
}