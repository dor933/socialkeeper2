{
  "expo": {
    "name": "social-keeper-client",
    "slug": "social-keeper-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/RandomImages/SocialKeeper.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/Images/RandomImages/Socialkeeper2new.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app accesses your calendar to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8b86c83a-ebbf-43be-9d49-3a4f984131da"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "The app accesses your calendar to let you share them with your friends.",
        "NSRemindersUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "package": "com.dor93.socialkeeperclient"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "socialkeeperclient",
    "extra": {
      "eas": {
        "projectId": "8b86c83a-ebbf-43be-9d49-3a4f984131da"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
